{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "baseUrl": ".",
    "paths": {
      "$types/*": ["src/@types/*"],
      "$components/*": ["src/components/*"],
      "$constants/*": ["src/constants/*"],
      "$functions/*": ["src/functions/*"],
      "$*": ["src/packlets/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["src"],
  "exclude": ["dist"]
}
