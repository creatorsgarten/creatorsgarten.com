{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "baseUrl": ".",
    "paths": {
      "$types/*": ["src/@types/*"],
      "$components/*": ["src/components/*"],
      "$constants/*": ["src/constants/*"],
      "$layouts/*": ["src/layouts/*"],
      "$plugins/*": ["src/plugins/*"],
      "$services/*": ["src/services/*"]
    },
  }
}
